{
    "name": "@endeavour/cli",
    "description": "Commands relating to endeavour frontend",
    "version": "1.3.3",
    "scripts": {
        "build": "forward-env rollup --configPlugin rollup-plugin-esbuild -c rollup.config.ts --silent",
        "build:watch": "rushx build",
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rushx build && oclif-dev manifest && oclif-dev readme",
        "version": "oclif-dev readme && git add README.md",
        "test": "",
        "lint": ""
    },
    "bin": {
        "dragon": "./bin/run"
    },
    "dependencies": {
        "@endeavour/ensure-certificate": "workspace:*",
        "@endeavour/library-builder": "workspace:*",
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/errors": "^1.3.5",
        "@oclif/parser": "^3.8.5",
        "@oclif/plugin-help": "^3",
        "tslib": "^2.5.0"
    },
    "devDependencies": {
        "@endeavour/configs": "workspace:*",
        "@endeavour/forward-env": "workspace:*",
        "@oclif/dev-cli": "^1",
        "@types/node": "18.15.3",
        "rollup": "^3.19.1",
        "ts-node": "^10.8.0",
        "typescript": "5.0.4",
        "esbuild": "~0.17.12",
        "rollup-plugin-esbuild": "^5.0.0"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "license": "UNLICENCED",
    "main": "dist/index.js",
    "oclif": {
        "commands": "./dist/commands",
        "bin": "dragon",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "types": "dist/index.d.ts"
}
