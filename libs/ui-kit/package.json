{
  "name": "@endeavour/ui-kit",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "storybook dev",
    "lint": "rushx lint:css --fix && rushx lint:js --fix",
    "lint:css": "stylelint '**/*.scss'",
    "lint:js": "eslint --cache --ext .tsx,.ts,.js src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-vite-library.es.js",
      "require": "./dist/react-vite-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/react-vite-library.umd.js",
  "module": "./dist/react-vite-library.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "react-router-dom": "^6.13.0",
    "@storybook/addon-essentials": "7.3.1",
    "@storybook/addon-interactions": "7.3.1",
    "@storybook/addon-links": "7.3.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.1",
    "@storybook/react": "7.3.1",
    "@storybook/react-vite": "7.3.1",
    "@storybook/testing-library": "0.2.0",
    "@storybook/addon-mdx-gfm": "7.3.1",
    "@storybook/csf-tools": "7.3.1",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "7.22.5",
    "sass": "~1.63.4",
    "@arthurgeron/eslint-plugin-react-usememo": "2.3.1",
    "@endeavour/configs": "workspace:*",
    "@types/node": "18.15.3",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "~8.42.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-scss": "^4.0.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "@storybook/addon-actions": "^7.0.4",
    "@storybook/addon-controls": "^7.0.4",
    "@storybook/addon-docs": "^7.0.4",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.0.4",
    "storybook": "7.3.1",
    "vite": "^4.3.9",
    "stylelint": "~15.7.0",
    "vite-plugin-dts": "3.5.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0",
    "autoprefixer": "^10.4.14"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "keywords": [],
  "license": "MIT"
}