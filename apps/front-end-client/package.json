{
    "name": "@endeavour/front-end-client",
    "version": "0.0.1",
    "homepage": "https://front-end-client.univet.com/",
    "type": "module",
    "scripts": {
        "build": "forward-env --after='tsc --noEmit --skipLibCheck' vite build",
        "build:watch": "rushx certs && vite",
        "serve": "rushx certs && sirv ./dist --host front-end-client.localhost --gzip --single --http2 --key ./.certs/server.key --cert ./.certs/server.crt",
        "certs": "dragon ensure-certificate front-end-client.localhost",
        "extract-translations": "formatjs extract \"./src/**/!(*.d).ts*\" --out-file ./src/lang/temp-en.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' --extract-source-location",
        "extract-compile": "formatjs compile './src/lang/temp-en.json' --ast --out-file ./src/lang/en.json && rm ./src/lang/temp-en.json",
        "extract-translations-and-compile": "rushx extract-translations && rushx extract-compile",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage",
        "lint": "rushx lint:css --fix && rushx lint:js --fix",
        "lint:css": "stylelint '**/*.scss'",
        "lint:js": "tsc --noEmit --skipLibCheck && eslint --cache --ext .tsx,.ts,.js src/"
    },
    "dependencies": {
        "@endeavour/ui-kit": "workspace:*",
        "@endeavour/keycloak-sdk": "workspace:",
        "@formatjs/intl-getcanonicallocales": "^2.2.1",
        "@formatjs/intl-locale": "^3.3.2",
        "@formatjs/intl-pluralrules": "^5.2.4",
        "@formatjs/intl-relativetimeformat": "^11.2.4",
        "@formkit/auto-animate": "^1.0.0-beta.6",
        "@hookform/resolvers": "~2.9.11",
        "@reduxjs/toolkit": "~1.9.5",
        "fast-deep-equal": "~3.1.3",
        "marked": "^4.3.0",
        "oidc-client-ts": "~2.2.4",
        "prop-types": "~15.8.1",
        "react": "^18.2.0",
        "react-currency-input-field": "3.6.11",
        "react-dom": "^18.2.0",
        "react-helmet-async": "~1.3.0",
        "react-hook-form": "^7.44.3",
        "react-hot-toast": "~2.4.1",
        "react-input-mask": "2.0.4",
        "react-intl": "^6.4.4",
        "react-oidc-context": "~2.2.2",
        "react-redux": "8.1.0",
        "react-router-dom": "^6.13.0",
        "vite-plugin-compression": "~0.3.6",
        "zod": "3.23.3",
        "axios": "~1.5.0",
        "@fullcalendar/react": "~6.1.8",
        "@fullcalendar/core": "~6.1.8",
        "@fullcalendar/daygrid": "~6.1.8",
        "@fullcalendar/interaction": "~6.1.8",
        "@fullcalendar/timegrid": "~6.1.8",
        "react-webcam": "~7.2.0"
    },
    "devDependencies": {
        "@endeavour/verification-integration": "workspace:",
        "@endeavour/cli": "workspace:*",
        "@endeavour/configs": "workspace:*",
        "@endeavour/forward-env": "workspace:*",
        "@arthurgeron/eslint-plugin-react-usememo": "2.3.1",
        "@babel/core": "^7.22.5",
        "@babel/plugin-transform-react-jsx": "7.22.5",
        "eslint": "~8.42.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "~7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.13",
        "eslint-plugin-unused-imports": "^2.0.0",
        "@faker-js/faker": "^8.4.1",
        "@formatjs/cli": "~6.1.3",
        "@mdx-js/react": "2.3.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-hooks": "~8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/marked": "~5.0.1",
        "@types/react": "^18.2.12",
        "@types/react-dom": "^18.2.5",
        "@types/react-input-mask": "3.0.2",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react": "^4.0.0",
        "@vitest/coverage-v8": "~0.33.0",
        "@vitest/ui": "~0.33.0",
        "autoprefixer": "^10.4.14",
        "esm-wallaby": "~3.2.25",
        "jest": "^29.7.0",
        "react-test-renderer": "~18.2.0",
        "sass": "~1.63.4",
        "sirv-cli": "~2.0.2",
        "stylelint": "~15.7.0",
        "typescript": "^5.3.3",
        "vite": "^4.3.9",
        "postcss-scss": "^4.0.1",
        "vite-plugin-mdx": "~3.5.11",
        "vitest": "~0.33.0"
    }
}
